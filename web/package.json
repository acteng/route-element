    "svelte": "^4.2.18",
    "svelte-check": "^3.8.0",
    "tslib": "^2.6.3",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5",
    "vite": "^4.5.3",
    "vite-plugin-wasm-pack": "0.1.11"
